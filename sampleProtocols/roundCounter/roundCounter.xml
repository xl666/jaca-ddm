<?xml version="1.0"?>
<config xmlns="http://uv.mx/hlimon"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://uv.mx/hlimon sys.xsd">

<name>Round Counter</name>

<agents>
  <agent>
    <program>contactPerson</program>
    <file>../sampleProtocols/roundCounter/contactPerson.asl</file>
  </agent>
   <agent>
    <program>worker</program>
    <file>../sampleProtocols/roundCounter/worker.asl</file>
  </agent>
   <agent>
    <program>roundController</program>
    <file>../sampleProtocols/roundCounter/roundController.asl</file>
  </agent>
</agents>

<params>
  <param>
    <name>Max Rounds</name>
    <type>int</type>
  </param>
  <param>
    <name>Init Percentage</name>
    <type>double</type>
  </param>
   <param>
    <name>Test Percentaje For Rounds</name>
    <type>double</type>
  </param>
   <param>
    <name>Change step</name>
    <type>double</type>
  </param>
</params>

</config>
